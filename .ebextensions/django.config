option_settings:
  aws:elasticbeanstalk:container:python:
    WSGIPath: billy_online_store.wsgi:application

# Configure static files
files:
  "/etc/nginx/conf.d/static.conf":
    mode: "000644"
    owner: root
    group: root
    content: |
      location /static/ {
          alias /var/app/current/static/;
      }
